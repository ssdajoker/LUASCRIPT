{
  "compilerOptions": {
    "target": "ES2020",
  "module": "Node16",
  "moduleResolution": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": false,
    "resolveJsonModule": true,
    "types": ["node"],
    "outDir": "dist"
  },
  "include": ["src/**/*.ts", "test/**/*.ts"]
}
